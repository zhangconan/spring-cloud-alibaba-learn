<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zkn.datasource.dao.first.FirstDAO">

    <sql id="Base_Column_List">
        id, name, gmt_create, gmt_modified, create_empid, last_modified_empid
    </sql>

    <select id="selectById" parameterType="java.lang.Long" resultType="java.lang.String">
        select name from demo where id = #{id}
    </select>

</mapper>
